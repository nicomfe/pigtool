{
  "name": "PIG",
  "version": "0.0.1",
  "description": "",
  "main": "index.js",
  "engines": {
    "node": ">=5.3.0",
    "npm": "^3.0.0"
  },
  "scripts": {
    "build": "npm run test && npm run dist:clean && npm run copy:static && npm run copy:htaccess && npm run copy:api && npm run compile",
    "dist:clean": "rm -rf dist && mkdir dist",
    "copy:static": "cp -r src/static/* dist",
    "copy:htaccess": "cp -r src/static/.htaccess dist && cp -r src/static/.htpasswd dist",
    "copy:api": "cp -r src/api/.htaccess dist",
    "compile": "NODE_ENV=production webpack -p --bail",
    "dev": "NODE_ENV=development node --harmony server",
    "dev:bs": "node --harmony server --with-browsersync",
    "prod": "node server/prod.js --port 3001",
    "flow-check": "flow check",
    "karma": "NODE_ENV=test karma start --single-run",
    "karma:watch": "NODE_ENV=test karma start --watch",
    "mocha": "NODE_ENV=test mocha --compilers js:babel-core/register --require mochaconfig.js 'src/**/__tests__/*.js'",
    "mocha:watch": "npm run mocha -- -w",
    "mocha-single": "NODE_ENV=test mocha --compilers js:babel-core/register --require mochaconfig.js",
    "mocha-single:watch": "npm run mocha-single -- -w",
    "lint": "eslint ./src/",
    "lint:fix": "eslint ./src/ --fix",
    "start": "npm run dev",
    "start:bs": "npm run dev:bs",
    "test": "npm run lint && npm run mocha",
    "test:watch": "npm run lint && npm run karma:watch",
    "translation": "NODE_ENV=translation node --harmony server",
    "sourcemapexplorer": "npm run build && source-map-explorer ./dist/app.*.js ./dist/app.*.js.map && source-map-explorer ./dist/vendor.*.js ./dist/vendor.*.js.map",
    "build-without-tests": "npm run dist:clean && npm run copy:static && npm run copy:htaccess && npm run copy:api && npm run compile"
  },
  "author": "Nicolas Fetter <nicofetter@gmail.com> (nicomfe)",
